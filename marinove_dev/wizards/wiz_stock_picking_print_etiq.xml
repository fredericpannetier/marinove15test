<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="wiz_stock_picking_print_etiq_form" model="ir.ui.view">
        <field name="name">wiz_stock_picking_print_etiq_form1</field>
        <field name="model">wiz_stock_picking_print_etiq</field>
        <field name="arch" type="xml">
            <tree string="picking_line" import="false" create="false" editable="bottom" default_order="move_name">
                <field name="move_name" readonly="1"></field>
                <field name="product_name" readonly="1"></field>
                <field name="etiquette_qty"></field>
                <field name="customer_name" readonly="1"></field>
                <field name="location_n_mention"></field>
                <field name="deliv_mention"></field>
                <field name="printer_id"></field>
                <field name="model_id"></field>
                <header>
                    <button string="Print Etiquette" type="object" name="print_etiq_from_picking" class="btn-primary" icon="fa-paper-plane text-success"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" icon="fa-times-circle text-danger"/>
                </header>
            </tree>
        </field>
    </record>

    <record id="action_wiz_stock_picking_print_etiq" model="ir.actions.act_window">
        <field name="name">Picking line</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">wiz_stock_picking_print_etiq</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="wiz_stock_picking_print_etiq_form"/>
        <field name="context">{'default_sale_order_line_id': context.get('sale_order_line_id',False),}</field>
        <field name="target">new</field>
    </record>
</odoo>